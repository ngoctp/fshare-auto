{
    "manifest_version": 2,
    "name": "Fshare Auto",
    "version": "1.0.0",
    "description": "Auto download from Fshare on viewing file details",
    "icons": {
        "16" : "icons/fa-16.png",
        "48" : "icons/fa-48.png",
        "128" : "icons/fa-128.png"
    },
    
    "content_scripts": [
        {
            "matches": [
                "https://www.fshare.vn/file/*"
            ],
            "js": [
                "lib/jquery-3.4.1.slim.min.js",
                "fshare.js"
            ],
            "run_at": "document_end"
        }
    ]

}
